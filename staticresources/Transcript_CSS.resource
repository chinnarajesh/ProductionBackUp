html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
    display: block;
}
body {
    line-height: 1;
}
ol, ul {
    list-style: none;
}
blockquote, q {
    quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
    content: '';
    content: none;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}

strong {
    font-weight:bold;   
}

/* GENERAL STYLES */
Html, body {
    font-family:'Gotham','Segoe UI',Trebuchet,Arial,Sans-Serif;
    color:#414042;
	height: 100%;
}

.mainPage {
	min-height: 100%;
	position: relative;
}

.tbody {
	height: 660px;
}

.tfooter {
	width: 100%;
	page-break-inside:avoid;
	page-break-after:auto;
	page-break-before:auto;
}

.clear-fix {    
    clear:both;
}

h1 {
    font-size:30px;
    font-weight:bold;
}

h2 {
    font-size:20px;
    text-transform:uppercase;
}

.wrapper {
    margin:0 0 0 0;  
    padding:0 0 0 0;
    font-size: 12px;
}

.section-divider {
    border-radius: 5px;
    border:1px solid #414042; 
	margin-bottom: 0px;
}

.callout-section {
    background-color:#75BEE9;   
}

.input-line {
    border-top:1px solid #414042;
    padding-top:5px;    
}


table.htmlDetailElementTable{
    float: left;
    max-width: 200px;
}

li.school-logo table.htmlDetailElementTable {
	float:none;
}

/* HIGH LEVEL STUDENT INFO STYLES */
.high-level-student-info {
	background-color:white;
	height: 70px;
	width:100%;
	font-size:12px;
}

.high-level-student-info ul {
	margin-left: 20px;
	margin-right: 15px;
	float: right;
	width: auto;
}

.high-level-student-info #school-info {
	float: left;
	display:inline-block;
	margin: 0px;
}

.high-level-student-info li {
	margin:5px 0 0 20px;
}

.high-level-student-info li span {
	font-weight: bold;
	margin-left: 5px;
}

.high-level-school-info {
	float: left;
	background-color: #75BEE9;
	color: #fff;
	width: 100%; 
	padding-top: 5px;
}

.high-level-school-info li {
	margin: 5px 0 0 0;
}

.high-level-school-info ul {
    float: left;
    margin-left: 15px;
    padding-bottom: 5px;
    min-width: 175px;
    max-width: 20%;
    width: auto;
    font-size: 9px;
    line-height: 1.25;
}

.student-name {
	height: auto;
	margin-left: 0px;
	font-size: 12px;
	font-weight: bold;
}

.student-name ul {
	float: left;
	width:200px;
	font-size:16px;
	font-weight:bold;
	padding:0px; 
	margin:0 0 0 5px;
}

.student-name li {
	margin:5px 0 0 0;
}

.year-summary {
	overflow: hidden;
    min-height: 280px;
    margin-bottom: 5px;
    
    float: left;
    width: 142px;
    border-radius: 5px;
    border: 1px solid #414042;   
    margin-left: 6px;    
}

.additional-info {
	width: 99.8%;
	padding: 0; 
	float: left;
	border: 1px solid #414042;
	font-size: 10px; 
	clear: both; 
	display: inline-block;
}

.grade-legend ul li.column0 { margin-left: 0px; }
.grade-legend ul li.column1 { margin-left: 100px; }
.grade-legend ul li.column2 { margin-left: 200px; }
.grade-legend ul li.column3 { margin-left: 300px; }
.grade-legend ul li.column4 { margin-left: 400px; }
.grade-legend ul li.column5 { margin-left: 500px; }

.grade-legend ul {
	max-height: 100px;
	width: 100%;
}

.grade-legend li {
	width: 100px;
	display: block;
	font-size:9px;
	line-height: 22px;
}

.grade-legend li.reset {
	margin-top: -66px;
}

.documentImage {
    padding-left:5px;
    overflow:hidden;
    height:65px;
    width:200px;
}
