    <validationRules>
        <fullName>CMs_cannot_update_all_student_info</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
($Profile.Id =&apos;00eE0000000e3RV&apos;),
ISCHANGED(Student_First_Name__c),
ISCHANGED(Student_Last_Name__c),
ISCHANGED(School_Year__c), 
ISCHANGED(Race__c), 
ISCHANGED(Grade__c), 
ISCHANGED(Student__c),
ISCHANGED(School__c),
ISCHANGED(Gender__c),
ISCHANGED(Ethnicity__c), 
ISCHANGED(Hispanic__c),
ISCHANGED(Lunch_Status__c),
ISCHANGED(Local_Student_ID__c))</errorConditionFormula>
        <errorMessage>Corps Members can only edit Strengths and Hobbies for their Students.  If you need to have another field edited please contact your School Admin.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exit_Date_and_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Active__c), Active__c=false, OR(ISBLANK(Exit_Date__c), ISBLANK(TEXT(Student_Exit_Reason__c))))</errorConditionFormula>
        <errorDisplayField>Active__c</errorDisplayField>
        <errorMessage>Please select Exit Date and Exit Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_future_birthdates</fullName>
        <active>true</active>
        <errorConditionFormula>Date_of_Birth__c  &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Date of Birth can not be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Negative_Service_Hours</fullName>
        <active>false</active>
        <description>Service Hours To Date must be non-negative.</description>
        <errorConditionFormula>Service_Hours_YTD__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Service_Hours_YTD__c</errorDisplayField>
        <errorMessage>Service Hours To Date must be non-negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_SysAdmin_can_change_Student_School</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISCHANGED(School__c), 
NOT(ISNEW()), 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Tier 1 Super Administrators&quot;,
$Profile.Name &lt;&gt; &quot;CY System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Int_Sys_Admin&quot; 
)</errorConditionFormula>
        <errorMessage>Only SysAdmin can change Student&apos;s School!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_specific_values_for_Gender</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL(Gender__c, &quot;Male&quot;),ISPICKVAL(Gender__c, &quot;Female&quot;),ISPICKVAL(Gender__c, &quot;Gender Neutral&quot;)))</errorConditionFormula>
        <errorMessage>Gender values are limited to Male, Female or Gender Neutral</errorMessage>
    </validationRules>
