    <validationRules>
        <fullName>Date_range_in_section</fullName>
        <active>true</active>
        <errorConditionFormula>OR( Date__c &lt; Section__r.Start_Date__c, Date__c &gt; Section__r.End_Date__c )</errorConditionFormula>
        <errorDisplayField>Date__c</errorDisplayField>
        <errorMessage>Date should be within the start and end of the section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_required</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL( Date__c)</errorConditionFormula>
        <errorMessage>Date can&apos;t be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dosage_denied</fullName>
        <active>true</active>
        <errorConditionFormula>Section__r.Account_Program__r.Program__r.SkipDosage__c</errorConditionFormula>
        <errorMessage>This program does not allow dosage to be logged</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Skill_Category_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK(Skill_Primary__c)), IF(Section__r.Account_Program__r.Program__r.Name == &apos;Tutoring: Math&apos;, TEXT(Skill_Primary__r.Category__c) &lt;&gt; &apos;Math&apos;, 
IF(Section__r.Account_Program__r.Program__r.Name == &apos;Tutoring: Literacy&apos;, TEXT(Skill_Primary__r.Category__c) &lt;&gt; &apos;Literacy&apos;,
TEXT(Skill_Primary__r.Category__c) &lt;&gt; &apos;SEL/Behavior and Attendance Interventions&apos;)))</errorConditionFormula>
        <errorDisplayField>Skill_Primary__c</errorDisplayField>
        <errorMessage>Skill category should be match</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Curriculum</fullName>
        <active>true</active>
        <errorConditionFormula>Section__r.RecordType.Name == &apos;Curriculum&apos; ||  ISPICKVAL(Section__r.In_After_School__c, &apos;Curriculum&apos;)</errorConditionFormula>
        <errorMessage>This is a curriculum section and is unable to add dosage.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
