        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

var message = sforce.apex.execute(&quot;Batch_CohortSnapshot&quot;, &quot;runCohortBatch&quot;, {});

if(message == &apos;success&apos;) {
    window.alert(&quot;Batch_CohortSnapshot are running.&quot;);
}
else if(message == &apos;error&apos;) {
    window.alert(&quot;There was an error when attempting to run the Batch_CohortSnapshot.&quot;);
}</url>
