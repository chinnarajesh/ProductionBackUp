    <fields>
        <fullName>Configure_Course_Catalog__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Course_Catalog__c !=&apos;&apos;, HYPERLINK(&apos;/apex/core_CourseCatalogSetup?setupId=&apos;+ Id,  Course_Catalog__r.Name, &quot;_top&quot;)  ,
HYPERLINK(&apos;/apex/core_CourseCatalogSetup?setupId=&apos;+ Id  ,   IMAGE(&apos;/img/feeds/follow12.png&apos;, &apos;Add&apos;),&quot;_top&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Configure Course Catalog</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
