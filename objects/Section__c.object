    <validationRules>
        <fullName>Cannot_change_existing_School_Program</fullName>
        <active>true</active>
        <errorConditionFormula>Account_Program__r.Program__c &lt;&gt;  Program__c</errorConditionFormula>
        <errorMessage>You may not change the School Program field when cloning a section. If you want to create a new section, please go to the Section tab and create &quot;New.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deactivation_Error</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Section_RT__c &lt;&gt; &apos;Unpublished&apos;,
OR(AND( Active__c , OR(NOT(TEXT( Section_Exit_Reason__c) =&quot;&quot;),NOT(ISBLANK( Section_Exit_Date__c )))), 

AND(NOT(ISNEW()),Active__c = false, OR(TEXT( Section_Exit_Reason__c) =&quot;&quot;,ISBLANK( Section_Exit_Date__c )))))</errorConditionFormula>
        <errorMessage>Deactivation error: either Active with exit date/reason or inactive without exit date/reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exit_Date_cant_be_in_future</fullName>
        <active>true</active>
        <errorConditionFormula>Section_Exit_Date__c &gt; NOW()</errorConditionFormula>
        <errorMessage>Exit date can&apos;t be in future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exit_reason_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Active__c),
Active__c = false,
 ISPICKVAL(Section_Exit_Reason__c, &apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Section_Exit_Reason__c</errorDisplayField>
        <errorMessage>Please select Exit Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Section_Exit_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Active__c), NOT(Active__c), ISNULL(Section_Exit_Date__c))</errorConditionFormula>
        <errorDisplayField>Section_Exit_Date__c</errorDisplayField>
        <errorMessage>Section_Exit_Date is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Section_Start_End_Date</fullName>
        <active>true</active>
        <description>Section Start Date cannot be greater than the End Date</description>
        <errorConditionFormula>Start_Date__c  &gt;  End_Date__c</errorConditionFormula>
        <errorMessage>Section Start Date cannot be greater than the End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Section_name_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Name) &amp;&amp;
NOT(OR($Profile.Name != &quot;Int_Sys_Admin&quot;, $Profile.Name != &quot;System Administrator&quot; )))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Section name can&apos;t be modified.</errorMessage>
    </validationRules>
