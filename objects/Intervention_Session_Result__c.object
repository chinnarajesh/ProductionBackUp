    <validationRules>
        <fullName>Prevent_Over_500_Session_Time</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_of_Time__c &gt;= 500</errorConditionFormula>
        <errorDisplayField>Amount_of_Time__c</errorDisplayField>
        <errorMessage>Session time for a day must be less then 500 minutes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Session_Time_Less_than_1</fullName>
        <active>true</active>
        <errorConditionFormula>Amount_of_Time__c &lt; 1</errorConditionFormula>
        <errorDisplayField>Amount_of_Time__c</errorDisplayField>
        <errorMessage>Session time cannot be less than 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Section_Validation</fullName>
        <active>true</active>
        <description>Validate that the Section associated with the related Student/Section record is the same as the Section  associated with the related Intervention Session record.</description>
        <errorConditionFormula>Student_Section__r.Section__c  &lt;&gt;  Intervention_Session__r.Section__c</errorConditionFormula>
        <errorMessage>Please verify that the Section record associated with the Student/Section record and Intervention Section record are the same.</errorMessage>
    </validationRules>
